{
  "name": "psyduck-frontend",
  "version": "1.0.0",
  "description": "Psyduck - Project-Based Learning Platform Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "frontend:dev": "vite --host",
    "backend:start": "npm --prefix backend start",
    "dev:all": "concurrently -n backend,frontend -c cyan,magenta \"npm run backend:start\" \"npm run frontend:dev\"",
    "build": "vite build",
    "build:prod": "vite build",
    "serve": "serve -s dist -l 5173",
    "backend:start:prod": "cross-env NODE_ENV=production node backend/server.js",
    "serve:all": "concurrently -n backend,frontend -c cyan,magenta \"npm run backend:start:prod\" \"npm run serve\"",
    "build:serve": "npm run build && npm run serve:all",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "prepare": "husky install",
    "postinstall": "npm --prefix backend install",
    "deploy:frontend": "npm run build",
    "deploy:backend": "cross-env NODE_ENV=production node backend/server.js",
    "deploy:pages": "npm run build && wrangler pages deploy dist --project-name psyduck-frontend"
  },
